<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="showPane" prefHeight="610.0" prefWidth="990.0" style="-fx-background-color: #f6f9fc;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nayana.controller.adminPage.AdShowNoticeController">
   <center>
      <VBox prefHeight="472.0" prefWidth="884.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="txtFieldTitle" prefHeight="23.0" prefWidth="670.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Label text="이름" />
                  <TextField fx:id="txtFieldWriter" />
                  <Label text="작성일" />
                  <TextField fx:id="txtFieldDate" />
                  <Label text="조회수" />
                  <TextField fx:id="txtFieldCnt" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="300.0" prefWidth="200.0">
               <children>
                  <TextArea fx:id="txtAreaContent" prefHeight="100.0" prefWidth="670.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="63.0" prefWidth="900.0" spacing="80.0">
               <children>
                  <Button fx:id="modifyBtn" mnemonicParsing="false" onAction="#modifyBtnClicked" style="-fx-background-color: #3d66ec; -fx-text-fill: white;" text="수정" />
                  <Button fx:id="deleteBtn" mnemonicParsing="false" onAction="#deleteBtnClicked" style="-fx-background-color: #3d66ec; -fx-text-fill: white;" text="삭제" />
                  <Button fx:id="returnBtn" mnemonicParsing="false" onAction="#returnBtnClicked" style="-fx-background-color: #3d66ec; -fx-text-fill: white;" text="목록으로" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <padding>
      <Insets left="60.0" right="60.0" top="40.0" />
   </padding>
</BorderPane>

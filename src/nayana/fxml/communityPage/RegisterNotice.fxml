<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="550.0" prefWidth="600.0" style="-fx-background-color: #f6f9fc;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nayana.controller.communityPage.RegisterNoticeController">
   <center>
      <VBox prefHeight="384.0" prefWidth="490.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="49.0" prefWidth="490.0">
               <children>
                  <Label style="-fx-font-size: 30;" text="공지사항 - 등록">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding></Label>
               </children>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="454.0">
               <children>
                  <Label text="제목">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="txtFieldTitle" prefHeight="15.0" prefWidth="410.0" />
               </children></HBox>
            <HBox prefHeight="289.0" prefWidth="490.0">
               <children>
                  <Label text="내용">
                     <padding>
                        <Insets left="5.0" right="5.0" top="130.0" />
                     </padding>
                  </Label>
                  <TextArea fx:id="txtAreaContent" prefHeight="300.0" prefWidth="410.0" />
               </children></HBox>
            <HBox alignment="CENTER" prefHeight="47.0" prefWidth="490.0" spacing="40.0">
               <children>
                  <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancelBtnClicked" text="취소" />
                  <Button fx:id="risisterBtn" mnemonicParsing="false" onAction="#resisterBtnClicked" style="-fx-background-color: #3d66ec; -fx-text-fill: white;" text="등록" />
               </children></HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </center>
   <padding>
      <Insets left="60.0" right="60.0" top="40.0" />
   </padding>
</BorderPane>
